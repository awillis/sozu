<?xml version="1.0" encoding="UTF-8"?>
<Config xmlns="http://pypi.org/project/xsdata" version="20.12">
  <Output wsdl="false" maxLineLength="79">
    <Package>sozu.data</Package>
    <Format>pydata</Format>
    <Structure>filenames</Structure>
    <DocstringStyle>reStructuredText</DocstringStyle>
    <CompoundFields>true</CompoundFields>
  </Output>
  <Conventions>
    <ClassName case="pascalCase" safePrefix="type"/>
    <FieldName case="snakeCase" safePrefix="value"/>
    <ModuleName case="snakeCase" safePrefix="mod"/>
    <PackageName case="snakeCase" safePrefix="pkg"/>
  </Conventions>
  <Aliases>
    <PackageName source="http://amekoshi.com/2020/tmt/model" target="tm"/>
    <PackageName source="http://amekoshi.com/2020/tmt/template" target="tt"/>
  </Aliases>
</Config>
